{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "To building a RESTful API with NodeJs &amp; TypeScript",
  "main": "index.js",
  "engines": {
    "node": "14.x",
    "npm": "^6.x"
  },
  "private": true,
  "scripts": {
    "postinstall": "npm-run-all --serial build",
    "clean": "rimraf dist/* tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build-ts": "tsc",
    "build": "npm-run-all --serial clean build-ts",
    "start": "node ./dist/index.js",
    "start:dev": "nodemon",
    "start:prod": "npm-run-all build start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samithks/express-typescript-starter.git"
  },
  "keywords": [],
  "author": "Samith K S",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/express-rate-limit": "5.1.1",
    "@types/morgan": "1.9.2",
    "@types/node": "14.14.37",
    "@types/response-time": "2.3.4",
    "@types/swagger-jsdoc": "6.0.0",
    "@types/swagger-ui-express": "4.1.2",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "5.2.6",
    "helmet": "4.6.0",
    "joi": "17.4.0",
    "morgan": "1.10.0",
    "openapi-types": "7.2.3",
    "response-time": "2.3.2",
    "rotating-file-stream": "2.1.5",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "4.1.6"
  },
  "bugs": {
    "url": "https://github.com/samithks/express-typescript-starter/issues"
  },
  "homepage": "https://github.com/samithks/express-typescript-starter#readme"
}
